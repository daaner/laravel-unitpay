{
    "name": "daaner/laravel-unitpay",
    "description": "UnitPay payments for Laravel",
    "keywords": [
        "laravel-unitpay",
        "unitpay",
        "payments"
    ],
    "homepage": "https://github.com/actionm/laravel-unitpay",
    "license": "MIT",
    "authors": [
        {
            "name": "actionm",
            "email": "actionmanager@gmail.com"
        },
        {
            "name": "daan",
            "email": "daan@ukr.net"
        },

    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "~5.5 || ~6.0 || ~7.0",
        "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0 || ~4.0 || ~5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ActionM\\UnitPay\\Facades\\": "src/Facades",
            "ActionM\\UnitPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActionM\\UnitPay\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ActionM\\UnitPay\\UnitPayServiceProvider"
            ],
            "aliases": {
                "UnitPay": "ActionM\\UnitPay\\Facades\\UnitPay"
            }
        }
    }
}
